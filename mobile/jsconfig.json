{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./src/",
    "paths": {
      "actions/*": ["actions/*"],
      "api/*": ["api/*"],
      "assets/*": ["assets/*"],
      "components/*": ["components/*"],
      "forms/*": ["forms/*"],
      "images/*": ["images/*"],
      "fonts/*": ["fonts/*"],
      "navigators/*": ["navigators/*"],
      "notify/*": ["notify/*"],
      "reducers/*": ["reducers/*"],
      "redux-constants/*": ["redux-constants/*"],
      "screens/*": ["screens/*"],
      "utils/*": ["utils/*"]
    }
  },
  "exclude": ["node_modules"]
}
