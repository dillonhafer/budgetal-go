{
  "name": "budgetal",
  "version": "1.17.1",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "node node_modules/jest/bin/jest.js",
    "test:watch": "node node_modules/jest/bin/jest.js --watch",
    "release": "node tasks/release.js",
    "upload": "node tasks/upload.js",
    "launch": "xcrun simctl launch booted 'host.exp.Exponent'",
    "terminate": "xcrun simctl terminate booted 'host.exp.Exponent'",
    "restart": "yarn terminate && yarn launch",
    "reset-password": "xcrun simctl openurl booted exp://127.0.0.1:19000/--/reset-password?reset_password_token=testtoken",
    "ver": "node tasks/version.js"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@shared": "file:../shared",
    "@types/styled-components": "^4.1.16",
    "expo": "^33.0.0",
    "expo-asset": "^5.0.1",
    "expo-blur": "~5.0.1",
    "expo-constants": "~5.0.1",
    "expo-font": "~5.0.1",
    "expo-permissions": "~5.0.1",
    "expo-secure-store": "~5.0.1",
    "lottie-react-native": "~2.6.1",
    "papaparse": "^4.3.6",
    "react": "16.8.3",
    "react-native": "https://github.com/expo/react-native/archive/sdk-33.0.0.tar.gz",
    "react-native-dropdownalert": "^3.3.0",
    "react-native-keyboard-aware-scroll-view": "^0.4.1",
    "react-native-modalbox": "^1.6.1",
    "react-native-snap-carousel": "^3.7.2",
    "react-native-swipeout": "^2.3.3",
    "react-navigation": "^3.11.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@types/react": "^16.8.19",
    "@types/react-native": "^0.57.60",
    "babel-preset-expo": "^5.1.1",
    "jest-expo": "^33.0.0",
    "typescript": "^3.4.5"
  }
}
